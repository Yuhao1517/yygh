<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yh.yygh.user.mapper.UserInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yh.yygh.user.pojo.UserInfo">
        <id column="id" property="id" />
        <result column="openid" property="openid" />
        <result column="nick_name" property="nickName" />
        <result column="phone" property="phone" />
        <result column="name" property="name" />
        <result column="certificates_type" property="certificatesType" />
        <result column="certificates_no" property="certificatesNo" />
        <result column="certificates_url" property="certificatesUrl" />
        <result column="auth_status" property="authStatus" />
        <result column="status" property="status" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="is_deleted" property="isDeleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, openid, nick_name, phone, name, certificates_type, certificates_no, certificates_url, auth_status, status, create_time, update_time, is_deleted
    </sql>

</mapper>
