<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yh.yygh.hosp.mapper.HospitalSetMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yh.yygh.hosp.pojo.HospitalSet">
        <id column="id" property="id"/>
        <result column="hosname" property="hosname"/>
        <result column="hoscode" property="hoscode"/>
        <result column="api_url" property="apiUrl"/>
        <result column="sign_key" property="signKey"/>
        <result column="contacts_name" property="contactsName"/>
        <result column="contacts_phone" property="contactsPhone"/>
        <result column="status" property="status"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="is_delected" property="isDelected"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, hosname, hoscode, api_url, sign_key, contacts_name, contacts_phone, status, create_time, update_time,
        is_delected
    </sql>

</mapper>
